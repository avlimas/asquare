###  http://data.europa.eu/eli/ontology#ResourceType
ontology:ResourceType owl:equivalentClass jolux:LegalResourceDocumentType ;
                      jolux:alignmentComment "jolux:LegalResourceDocumentType"@fr .


###  http://data.legilux.public.lu/resource/ontology/jolux#Act
jolux:Act rdf:type owl:Class ;
          rdfs:subClassOf jolux:LuxActAndEULegalResource ,
                          jolux:NationalLegalResource ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty jolux:basisFor ;
                            owl:someValuesFrom jolux:Act
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty jolux:transpositionCompletion ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass jolux:TranspositionCompletionStatus
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty jolux:needForImplementingRegulation ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:boolean
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty jolux:conditionalApplicability ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:boolean
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty jolux:conditionalApplicabilityRemark ;
                            owl:maxCardinality "1"^^xsd:nonNegativeInteger
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty jolux:dateApplicability ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:date
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty jolux:dateEndApplicability ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:date
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty jolux:dateEntryInForce ;
                            owl:maxCardinality "1"^^xsd:nonNegativeInteger
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty jolux:dateNoLongerInForce ;
                            owl:maxCardinality "1"^^xsd:nonNegativeInteger
                          ] ;
          owl:disjointWith jolux:Code ,
                           jolux:Collection ,
                           jolux:Consolidation ,
                           jolux:Memorial ;
          rdfs:comment "Actes."@fr .


