                        rdfs:subPropertyOf ontology:relevant_for ;
                        rdfs:domain jolux:LegalResource ;
                        rdfs:range jolux:AdministrativeArea ;
                        jolux:alignmentComment "pas d'alignement direct, mais alignement en positionnant jolux:applicabilityArea comme sous propriété de eli:relevantFor"@fr ;
