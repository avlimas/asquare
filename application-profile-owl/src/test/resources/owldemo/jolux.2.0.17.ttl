                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty jolux:basicAct ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass jolux:Act
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty jolux:dateApplicability ;
                                      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onDataRange xsd:date
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty jolux:dateEndApplicability ;
                                      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onDataRange xsd:date
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty jolux:version ;
                                      owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                    ] ;
                    owl:disjointWith jolux:Memorial ;
                    rdfs:comment "Consolidations d'une acte de base au fur et à mesure de ses modifications."@fr .


