jolux:basedOn rdf:type owl:ObjectProperty ;
              owl:inverseOf jolux:basisFor ;
              rdfs:domain jolux:Act ;
              rdfs:range jolux:LuxActAndEULegalResource ;
              rdfs:comment """Indique l'acte national ou le réglement européen sur lequel l'acte décrit s'appuie. Par exemple un règlement d'exécution national s'appuie
            sur sa loi de base.
        """@fr ;
              skos:altLabel "Règlement d'exécution de"@fr .


###  http://data.legilux.public.lu/resource/ontology/jolux#basicAct
jolux:basicAct rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf jolux:consolidates ;
               owl:inverseOf jolux:isBasicActOf ;
               rdfs:domain jolux:Consolidation ;
               rdfs:range jolux:BaseAct ;
               jolux:alignmentComment "pas d'alignement eli"@fr ;
               rdfs:comment "Indique l'acte de base consolidé par la consolidation décrite." ;
               skos:altLabel "Texte consolidé"@fr .


###  http://data.legilux.public.lu/resource/ontology/jolux#basisFor
jolux:basisFor rdf:type owl:ObjectProperty ;
               rdfs:domain jolux:LuxActAndEULegalResource ;
               rdfs:range jolux:Act ;
               rdfs:comment """Indique le ou les resources légales qui ont comme base la resource décrite. Par exemple, un acte de base est la base pour des règlements d'exécution
            (inféré).
        """@fr ;
               skos:altLabel "A pour règlement d'exécution"@fr .


###  http://data.legilux.public.lu/resource/ontology/jolux#changedBy
jolux:changedBy rdf:type owl:ObjectProperty ;
                owl:inverseOf jolux:changes ;
                rdfs:domain jolux:Act ;
                rdfs:range jolux:Act ;
                rdfs:comment """Indique la ressource légale qui modifie la ressource légale décrite, la propriété n'est pas utilisée directement, ce sont les sous propriétés qui sont
            renseignées (inféré).
        """@fr ;
                skos:altLabel "Modifié par"@fr .


###  http://data.legilux.public.lu/resource/ontology/jolux#changes
jolux:changes rdf:type owl:ObjectProperty ;
              rdfs:domain jolux:Act ;
              rdfs:range jolux:Act ;
              rdfs:comment """Indique la ressource légale modifiée par la ressource légale décrite. Pas utilisés directement, super propriété de \"repeals\", \"modifies\", \"rectifies\"
            (inféré).
        """@fr ;
              skos:altLabel "Change"@fr .


