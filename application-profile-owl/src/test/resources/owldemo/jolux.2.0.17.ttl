                           rdfs:domain jolux:Manifestation ;
                           rdfs:range owl:Thing ;
                           jolux:alignmentComment "alignement sur eli:is_exemplified_by qui est la super propriété"@fr ;
                           rdfs:comment "URL du ou des annexes du fichier représenté par la manifestation décrite."@fr ;
                           skos:altLabel "Annexe"@fr .


###  http://data.legilux.public.lu/resource/ontology/jolux#answerToOpinionRequest
jolux:answerToOpinionRequest rdf:type owl:ObjectProperty ;
                             rdfs:domain jolux:Opinion ;
                             rdfs:range jolux:OpinionRequest ;
                             rdfs:comment "Indication de la réquête à laquelle cette avis répond"@fr ;
